@model EducationCommunity.Models.Account
@{
    ViewBag.Title = "login";
}
<link href="~/Content/bootstrap.css" rel="stylesheet" />
<h2>login</h2>
<br />


<div class="container">
<div class="main">
    
<div class="col-md-4">
    
    <div class="form-horizontal">
   
<form class="sign-in" method="post" action="loginTry">
    @Html.ValidationSummary()
        <label for="inputEmail" class="sr-only" >Email address</label>
 
    
      <input type="email" name="AccEmail" class="form-control" placeholder="Enter your Email Address" required />
            <br />
    <label for="inputPassword" class="sr-only" >Password</label>
    <input type="password" name="AccPassword" class="form-control" placeholder="Enter your password here" required  />
    <br /> 
    <div class="checkbox">
       &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp <input type="checkbox" value="remember-me" />Remember me
    <br/>
        <br />
     
        <button class="btn btn-primary btn-block" type="submit">Sign in</button>
         
         </div>
</form>

   </div>
    </div>
    </div>
    </div>
